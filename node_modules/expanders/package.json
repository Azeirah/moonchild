  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "init-repo": "mkdir bin dist lib third_party; git remote rm origin; npm init && git reset --soft ceac6c && git add . && git commit --amend -m 'Initial commit.'",
    "build": "browserify -s expanders -o dist/expanders-bundle.js index.js",
    "build-examples": "browserify -s environments -o dist/environments-bundle.js examples/environments/environments.js",
    "watch": "watchify -s expanders -o dist/expanders-bundle.js index.js",
    "test": "tape test/*.js",
    "prepublish": "npm run test && npm run lint && npm run build",
    "lint": "jshint *.js test/*.js"
  },
  "description": "An implementation of Expanders (Warth et al, 2006) for JavaScript.",
